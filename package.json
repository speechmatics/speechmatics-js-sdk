{
  "name": "speechmatics",
  "version": "3.1.1",
  "description": "Speechmatics Javascript Libraries",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build:cjs": "./build.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "npm run build:clean && npm-run-all --parallel build:cjs build:types",
    "test": "jest",
    "prepare": "npm run build",
    "check": "npx biome check .",
    "format": "npx biome format --write ."
  },
  "precommit": [
    "check"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speechmatics/speechmatics-js-sdk.git"
  },
  "keywords": [
    "speechmatics",
    "speech",
    "recognition",
    "transcription",
    "speech to text",
    "text to speech",
    "speechmatics-js"
  ],
  "homepage": "https://github.com/speechmatics/speechmatics-js-sdk#readme",
  "author": "Speechmatics",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@biomejs/biome": "1.0.0",
    "@types/chrome": "^0.0.246",
    "@types/jest": "^29.4.2",
    "@types/node": "^18.15.3",
    "@types/ws": "^8.5.5",
    "babel-jest": "^29.5.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "esbuild": "0.18.12",
    "glob": "^10.3.3",
    "jest": "^29.5.0",
    "jest-websocket-mock": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "bufferutil": "^4.0.7",
    "events": "^3.3.0",
    "utf-8-validate": "^6.0.3",
    "ws": "^8.13.0",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  }
}
